{
	"manifest_version": 3,
	"name": "Edge Google Tab",
	"version": "1.0",
	"description": "Replaces new tab with Google while leaving typing in the URL bar",
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	}
}